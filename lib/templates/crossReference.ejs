<%
for (var idx = 0; idx < refs.length; idx++) {
  var ref = refs[idx];
  var trimmed = ref.split(':').pop();

  if (ref.includes('fig:')) {
    -%>@<img>{<%= trimmed -%>}<%
  } else if (ref.includes('tbl:')) {
    -%>@<table>{<%= trimmed -%>}<%
  } else if (ref.includes('code:')) {
    -%>@<list>{<%= trimmed -%>}<%
  } else {
    -%>@<raw>{|latex|\ref{<%= refs[idx] -%>\}}<%
  }
}
-%>
<%
if (cites.length !== 0) {
  -%>@<raw>{|latex|\cite{<%= cites.join(',') -%>\}}<%
}
-%>
