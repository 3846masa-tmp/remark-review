<%
for (var idx = 0; idx < node.refs.length; idx++) {
  var ref = node.refs[idx];
  var trimmed = ref.split(':').pop();

  if (ref.includes('fig:')) {
    -%>@<img>{<%= trimmed -%>}<%
  } else if (ref.includes('tbl:')) {
    -%>@<table>{<%= trimmed -%>}<%
  } else if (ref.includes('code:')) {
    -%>@<list>{<%= trimmed -%>}<%
  } else {
    -%>@<raw>{|latex|\ref{<%= ref -%>\}}<%
  }
}
-%>
<%
if (node.cites.length !== 0) {
  -%>@<raw>{|latex|\cite{<%= node.cites.join(',') -%>\}}<%
}
-%>
